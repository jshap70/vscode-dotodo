{
    "name": "dotodo",
    "displayName": "dotodo",
    "description": "todooo todooo todooo todooo todooooooooooo",
    "version": "0.0.1",
    "publisher": "jshap70",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        // "*",
        "onView:dotodoExplorer"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "views": {
            "explorer": [
                {
                    "id": "dotodoExplorer",
                    "name": "Todo's"
                }
            ]
        },
        "commands": [
            {
                "command": "dotodo.jumpTo",
                "title": "Jump to document/line"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "dotodo.jumpTo",
                    "when": "false"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
